@using Domain._DTO.Event
@model IList<EventDto>

@{
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
    ViewData["Title"] = "UpcomingEvents";
}

<div class="conatiner-fluid m-4">
    <div class="row">
        @foreach(var upcomingevent in  Model){
            <div class="col-md-3">
                <div class="card">
                    @upcomingevent.Name
                </div>
            </div>
        }
    </div>
</div>